   <children>
      <Button fx:id="customerButton" layoutX="150.0" layoutY="125.0" mnemonicParsing="false" onAction="#goToCustomer" prefHeight="75.0" prefWidth="150.0" text="Records" />
      <Button fx:id="appointmentsButton" layoutX="300.0" layoutY="125.0" mnemonicParsing="false" onAction="#goToAppointments" prefHeight="75.0" prefWidth="150.0" text="Appointments" />
      <Button fx:id="calendarButton" layoutX="150.0" layoutY="200.0" mnemonicParsing="false" onAction="#goToCalendar" prefHeight="75.0" prefWidth="150.0" text="Calendar" />
      <Button fx:id="reportsButton" layoutX="300.0" layoutY="200.0" mnemonicParsing="false" onAction="#goToReports" prefHeight="75.0" prefWidth="150.0" text="Reports" />
   </children>
</AnchorPane>
