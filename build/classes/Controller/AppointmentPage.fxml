<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AppointmentPageController">
   <children>
      <TableView fx:id="AppointmentTable" layoutY="60.0" prefHeight="200.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="AppointmentID" prefWidth="103.0" text="Appointment ID" />
          <TableColumn fx:id="CustomerID" prefWidth="98.0" text="Customer ID" />
            <TableColumn fx:id="Type" prefWidth="127.0" text="Type" />
            <TableColumn fx:id="Start" prefWidth="134.0" text="Start" />
            <TableColumn fx:id="End" prefWidth="137.0" text="End" />
        </columns>
      </TableView>
      <Button fx:id="addButton" layoutY="280.0" mnemonicParsing="false" onAction="#add" prefHeight="75.0" prefWidth="150.0" text="Add" />
      <Button fx:id="updateButton" layoutX="150.0" layoutY="280.0" mnemonicParsing="false" onAction="#update" prefHeight="75.0" prefWidth="150.0" text="Update" />
      <Button fx:id="deleteButton" layoutX="300.0" layoutY="280.0" mnemonicParsing="false" onAction="#delete" prefHeight="75.0" prefWidth="150.0" text="Delete" />
      <Button fx:id="backButton" layoutX="450.0" layoutY="280.0" mnemonicParsing="false" onAction="#goToMain" prefHeight="75.0" prefWidth="150.0" text="Back" />
   </children>
</AnchorPane>
