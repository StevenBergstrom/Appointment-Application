<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.CustomerPageController">
   <children>
      <Button fx:id="addButton" layoutY="262.0" mnemonicParsing="false" onAction="#add" prefHeight="75.0" prefWidth="150.0" text="Add" />
      <Button fx:id="deleteButton" layoutX="300.0" layoutY="262.0" mnemonicParsing="false" onAction="#delete" prefHeight="75.0" prefWidth="150.0" text="Delete" />
      <Button fx:id="backButton" layoutX="450.0" layoutY="262.0" mnemonicParsing="false" onAction="#goToMain" prefHeight="75.0" prefWidth="150.0" text="Back" />
      <Button fx:id="updateButton" layoutX="150.0" layoutY="262.0" mnemonicParsing="false" onAction="#update" prefHeight="75.0" prefWidth="150.0" text="Update" />
      <TableView fx:id="CustomerTable" layoutX="100.0" layoutY="47.0" prefHeight="200.0" prefWidth="400.0">
        <columns>
          <TableColumn fx:id="CustomerID" prefWidth="199.0" text="Customer ID" />
          <TableColumn fx:id="CustomerName" prefWidth="200.0" text="Customer Name" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
