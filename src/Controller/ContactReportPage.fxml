<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ContactReportPageController">
   <children>
      <TableView fx:id="ContactReportTable" layoutX="50.0" layoutY="100.0" prefHeight="200.0" prefWidth="500.0">
         <columns>
            <TableColumn fx:id="AppointmentIDColumn" prefWidth="104.0" text="Appointment ID" />
            <TableColumn fx:id="ContactColumn" prefWidth="79.0" text="Contact" />
            <TableColumn fx:id="StartColumn" prefWidth="154.0" text="Start" />
            <TableColumn fx:id="EndColumn" prefWidth="162.0" text="End" />
         </columns>
      </TableView>
      <ComboBox fx:id="ContactDropDown" layoutX="225.0" layoutY="40.0" onAction="#selectContact" prefWidth="150.0" />
      <Button fx:id="BackButton" layoutX="225.0" layoutY="311.0" mnemonicParsing="false" onAction="#goToReports" prefHeight="75.0" prefWidth="150.0" text="Back" />
   </children>
</AnchorPane>
